configuration {
  location: 2;
  yoffset: 75;
}

* {
    padding: 5px 10px;
    font: "Font Awesome 20";
}

#window {
  width: 395px;
  height: 80px;
  border-radius: 20px;
  children: [ horibox ];
}
#horibox {
  orientation: horizontal;
  children: [ listview ];
}
#listview {
  layout: horizontal;
}
#element {
  padding: 10px 30px 0px ;
}
#element selected {
  text-color: #81a1c1;
}
